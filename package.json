{
  "name": "sls-digital-edition-frontend",
  "version": "2.5.0",
  "description": "Angular frontend app for the SLS Digital Edition Platform",
  "homepage": "https://github.com/slsfi/digital-edition-frontend-ng#readme",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development,sv",
    "start:fi": "ng serve --configuration=development,fi",
    "extract-i18n": "ng extract-i18n --sort idAsc",
    "generate-routes": "node ./prebuild-generate-routes.js",
    "generate-sitemap": "node ./prebuild-generate-sitemap.js",
    "generate-static-collection-menus": "node ./prebuild-generate-static-collection-menus.js",
    "build:ssr": "npm run generate-routes && ng build --configuration production && ng run app:server:production && node ./postbuild-copy-files.js",
    "serve:ssr": "node dist/app/proxy-server.js",
    "ssr-start": "npm run build:ssr && npm run serve:ssr",
    "test:routes-parser": "node ./scripts/test-prebuild-generate-routes.js",
    "test:ssr:smoke": "node ./scripts/test-ssr-smoke.js",
    "bench:ssr": "node ./scripts/benchmark-ssr.js",
    "bench:ssr:build": "npm run build:ssr && npm run bench:ssr",
    "gzipper": "gzipper",
    "compress": "gzipper compress ./dist/app/browser --include html,htm,xml,txt,css,js,svg,ico,json,ttf,otf --threshold 1300 --gzip-level 9 --remove-larger"
  },
  "dependencies": {
    "@angular/animations": "^20.3.17",
    "@angular/common": "^20.3.17",
    "@angular/compiler": "^20.3.17",
    "@angular/core": "^20.3.17",
    "@angular/forms": "^20.3.17",
    "@angular/platform-browser": "^20.3.17",
    "@angular/platform-server": "^20.3.17",
    "@angular/router": "^20.3.17",
    "@angular/ssr": "^20.3.18",
    "@ionic/angular": "8.2.2",
    "@ionic/angular-server": "8.2.2",
    "express": "^5.2.1",
    "htmlparser2": "^10.1.0",
    "ionicons": "^7.4.0",
    "marked": "^17.0.3",
    "marked-custom-heading-id": "^2.0.16",
    "marked-footnote": "^1.4.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.16.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.18",
    "@angular/cli": "^20.3.18",
    "@angular/compiler-cli": "^20.3.17",
    "@angular/language-service": "^20.3.17",
    "@angular/localize": "^20.3.17",
    "@types/express": "^5.0.6",
    "@types/jasmine": "~5.1.15",
    "@types/node": "^22.19.11",
    "browser-sync": "^3.0.4",
    "gzipper": "^8.2.1",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.2.0",
    "ng-extract-i18n-merge": "^2.15.1",
    "typescript": "~5.8.3"
  }
}
